@model IEnumerable<FeemanagementSystem.Models.ReceiptPrint>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.PrintTime)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PrintDate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PrintUser)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.RidNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.PrintTime)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PrintDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PrintUser.UserId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.RidNavigation.Rid)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.PrintId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.PrintId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.PrintId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
